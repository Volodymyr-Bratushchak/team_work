# EditorConfig is awesome: https://EditorConfig.org

# Top-most EditorConfig file
root = true

# Format C# files
[*.cs]
indent_style = space
indent_size = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = false

# Set maximum line length to 120 characters
max_line_length = 120
